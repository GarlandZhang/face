<p> <%= link_to "back to dashboard", dashboard_path(@user) %> </p>
<img src="<%= image.url %>">
<h2>People</h2>
<ul>
  <% @people.each do |person| %>
    <li>
      <%= link_to person.name, person_path(person) %>
    </li>
  <% end %>
</ul>
<h4>Tags</h4>
<ul>
  <% @object_tags.each do |tag| %>
    <li>
      <%= tag.name %>
    </li>
  <% end %>
</ul>