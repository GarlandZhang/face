<div>
  <%= form_tag(search_path, {:method => :get}) %>
  <p>Search <%= text_field_tag :names %></p>
  <%= text_field_tag(:id, @user.id, {type: :hidden}) %>
  <%= submit_tag 'Search' %>
</div>
<p> Hello <%= @user.username %></p>
<%= link_to "Add image", add_image_path(@user)%>
<h3>Images</h3>
<% @images.each do |image| %>
  <div>
    <%= image_tag image.image %>
  </div>
<% end %>

<h3><%= @person_group.name %></h3>
<h4>People</h4>
<ul>
  <% @people.each do |person| %>
      <li>
        <%= link_to person.name, person_path(person) %>
      </li>
  <% end %>
</ul>